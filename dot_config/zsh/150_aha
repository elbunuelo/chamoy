addToPath() {
 export PATH=$PATH:$1
}

alias pnpm="wo pnpm"
alias nuke="wo rm -rf node_modules packages/*/node_modules .turbo-cache"
alias aha-start-server="overmind s --any-can-die"

which aha > /dev/null 2>&1 && alias aha-cli=$(which aha)
[ -f "$HOME/Projects/aha-dev-cli/aha" ] && alias aha=~/Projects/aha-dev-cli/aha

addToPath "$HOME/Projects/ops/bin"
